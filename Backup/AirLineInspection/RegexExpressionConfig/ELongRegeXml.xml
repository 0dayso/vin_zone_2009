<?xml version="1.0" encoding="utf-8" ?>
<RegexExpression>  
  <!--获取每个table的数据-->
  <SingleRow>
    <![CDATA[(<TABLE\s+class=fltable)((?s).+?)(</TABLE>)]]>
    <!--<![CDATA[(<TABLE\s+class=fltable)((?s).=""+)(</TABLE>)]]>-->
    </SingleRow>
  <!--获取rule中的数据-->
  <RuleData>
    <![CDATA[(?<=(<A\s.+method="flightrule"\s+rule="))((?s).+?)(?=">)]]>
  </RuleData>
  <!--获取rule中的年月日-->
  <YearMonthDay>
    <![CDATA[\d{4}(/\d{1,2})+]]>
  </YearMonthDay>
  <!--获取出发城市-->
  <DepartureCity>
    <![CDATA[(?<=(</SPAN>))([\u4e00-\u9fa5]+)(?=(<BR>))]]>
  </DepartureCity>
  <!--获取到达城市-->
  <ArrivalCity>
    <![CDATA[((?<=(</SPAN>))(.+)(?=(<A\s.+method="StopOver".+>))]]>
  </ArrivalCity>
  <!--获取起飞时间-->
  <DepartureTime>
    <![CDATA[(?<=(<SPAN\s+class=ftime>))([\d:\d]+)(?=</SPAN>)]]>
    <!--<![CDATA[\d{4}(/\d{1,2})+\s(\d{1,2}:?)+]]>-->
  </DepartureTime>
  <!--获取到达时间-->
  <ArrivalTime>
    <![CDATA[(?<=(<SPAN>))(.+)(?=(</SPAN>))]]>
  </ArrivalTime>
  <!--获取航空公司-->
  <AirLine>
    <![CDATA[(?<=(<TD\s+class=cols04>[^A-z]+))([0-9A-Z]{2})]]>
</AirLine>
  <!--获取航班号-->
  <FlightNO>
    <![CDATA[(?<=(<TD\s+class=cols04>[^A-Z0-9]+))([A-Z0-9]+)]]>
  </FlightNO>
  <!--获取折扣-->
  <Discount>
    <![CDATA[(?<=(<SPAN\s+class=tax>))([\d(.\d)?折|特价]+)]]>
  </Discount>
  <!--获取机建-->
  <Airport>
    <![CDATA[]]>
  </Airport>
  <!--获取燃油-->
  <Fuel>
    <![CDATA[]]>
  </Fuel>
  <!--获取机建、燃油-->
  <EAirportFuel>
    <![CDATA[(?<=(<TD\s+class=cols05>))(\d+)(?=(</TD>))]]>
  </EAirportFuel>
  <!--获取舱位-->
  <Cabin>
    <![CDATA[]]>
  </Cabin>
  <!--获取机型-->
  <FlightType>
    <!--<![CDATA[(?<=(<A\s.+method="PlanType">[^0-9]+))([0-9]+)]]>-->
    <![CDATA[(?<=\|)([A-Za-z]+[0-9]+[A-Za-z0-9]*|[0-9]+[A-Za-z]+[A-Za-z0-9]*|[\d]{3})(?=\|)]]>
  </FlightType>  
  <!--获取价格-->
  <TicketPrice>
    <![CDATA[(?<=(<SPAN\sclass=price>+[^0-9]+))([0-9]+)]]>    
  </TicketPrice>
  <!--获取退改签规则-->
  <ChangeRule>
    <![CDATA[]]>
  </ChangeRule>
  <!--获取城市对值-->
  <City>
    <![CDATA[(?<=(</SPAN>))([\u4e00-\u9fa5]{2,})]]>
  </City>
  
</RegexExpression>
