<?xml version="1.0" encoding="utf-8" ?>
<RegexExpression>
    <!--获取每行数据-->
    <SingleRow>
      <![CDATA[(<TBODY\s+data=")((?s).+?)(</TBODY>)]]>
    </SingleRow>
    <!--获取航空公司-->
    <AirLine>
      <![CDATA[(?<=\|)([A-Z0-9]{2})(?=\|)]]>
    </AirLine>
    <!--获取机建-->
    <Airport>
      <![CDATA[]]>
    </Airport>
    <!--获取到达城市-->
    <ArrivalCity>
      <![CDATA[(?<=\|)([A-Z]{3})(?=\|)]]>
    </ArrivalCity>
    <!--获取到达时间-->
    <ArrivalTime>
      <![CDATA[\d{4}(-\d{1,2})+\s(\d{1,2}:?)+]]>
    </ArrivalTime>
    <!--获取舱位-->
    <Cabin>
      <![CDATA[(?<=(<SPAN.+mod_jmpinfo_content.+/))[A-Z]\d?(?=(</SPAN>))]]>
    </Cabin>
    <!--获取退改签规则-->
    <ChangeRule>
      <![CDATA[(?<=(<SPAN((?s).)+flight_policy_tab((?s).)+"))((?s).+)(?=(">退改签))]]>
    </ChangeRule>
    <!--获取出发城市-->
    <DepartureCity>
      <![CDATA[(?<=\|)([A-Z]{3})(?=\|)]]>
    </DepartureCity>
    <!--获取起飞时间-->
    <DepartureTime>
      <![CDATA[\d{4}(-\d{1,2})+\s(\d{1,2}:?)+]]>
    </DepartureTime>
    <!--获取折扣-->
    <Discount>
      <![CDATA[(?<=\|)(\d+\.\d+)]]>
    </Discount>
    <!--获取航班号-->
    <FlightNO>
      <![CDATA[(?<=(<SPAN\s+class=flight_airline>))(.+)(?=(</SPAN>))]]>
    </FlightNO>
    <!--获取机型-->
    <FlightType>
      <![CDATA[(?<=(<SPAN.+CraftType.+>))(.+)(?=(</SPAN>))]]>
    </FlightType>
    <!--获取燃油-->
    <Fuel>
      <![CDATA[]]>
    </Fuel>
    <!--获取票价-->
    <TicketPrice>
      <![CDATA[(?<=\|)(\d{3,6})(?=\|)]]>
    </TicketPrice>
    <!--获取Y舱价格-->
    <Yprice>
      <![CDATA[(?<=(<SPAN((?s).)+经济舱全价：))(\d+)(?=(<BR/>))]]>
    </Yprice>
    <!--/////////////////////////////////////////////////////////////////////////////////////////-->
    <!--获取Tbody元素中Data属性的值-->
    <TbodyData>
      <![CDATA[(?<=(<TBODY\s+data="))(.+)(?=(">))]]>
    </TbodyData>
    <!--获取城市对值-->
    <City>
      <![CDATA[(?<=\|)([A-Z]{3})(?=\|)]]>
    </City>
    <!--获取日期对值-->
    <Date>
      <![CDATA[\d{4}(-\d{1,2})+\s(\d{1,2}:?)+]]>
    </Date>
    <!--获取餐食-->
    <Meal>
      <![CDATA[(?<=\|)([A-Z])(?=\|)]]>
    </Meal>
    <!--获取机建燃油-->
    <AirportFuel>
      <![CDATA[(?<=(<P>机建＋燃油：))(.+)(?=(元\s*\|))]]>
    </AirportFuel>
    <!--获取飞行时长-->
    <FlightInterval>
      <![CDATA[(?<=(飞行时间：))(.+)(?=(</P>))]]>
    </FlightInterval>
  <!--获取某个航班的所有舱位信息-->
  <AllCabinInfomation>
    <![CDATA[(?<=onclick="SelectFlight\('{0}',)(.+)(?=\)"\s+/>)]]>
  </AllCabinInfomation>
  <!--获取某个航班的舱位-->
  <OtherCanbin>
    <![CDATA[(?<=')([A-Z])(?=')]]>
  </OtherCanbin>
  <!--获取某个航班舱位的价格-->
  <OtherCanbinPrice>
    <![CDATA[(?<=')(\d{3,}(.\d+)?)(?=')]]>
  </OtherCanbinPrice>
</RegexExpression>